{
  "name": "allow-scripts",
  "version": "0.0.0",
  "description": "Execute allowed `npm install` lifecycle scripts",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/dominykas/allow-scripts.git"
  },
  "scripts": {
    "lint-commits": "commitlint --from=${TRAVIS_BRANCH:-origin/master}",
    "test": "lab -L -t 100 -m 5000 && npm run lint-commits"
  },
  "bin": {
    "allow-scripts": "./bin/allow-scripts.js"
  },
  "author": "Dominykas Blyžė <hello@dominykas.com>",
  "license": "MIT",
  "dependencies": {
    "ansicolors": "0.3.x",
    "libnpm": "3.x.x",
    "semver": "7.x"
  },
  "devDependencies": {
    "@commitlint/cli": "8.x.x",
    "@commitlint/config-conventional": "8.x.x",
    "@hapi/code": "6.x.x",
    "@hapi/lab": "20.x.x",
    "mkdirp": "0.5.x",
    "rimraf": "3.x.x",
    "semantic-release": "15.x.x",
    "sinon": "7.x.x"
  },
  "files": [
    "bin",
    "lib"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
